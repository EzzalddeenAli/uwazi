            "schema": {
              "title": "Page",
              "type": "object",
              "additionalProperties": false,
              "properties": {
                "limit": {
                  "type": "number"
                },
                "offset": {
                  "type": "number"
                }
              }
            },
            "style": "deepObject"
          },
          {
            "in": "query",
            "name": "filter",
            "schema": {
              "type": "object",
